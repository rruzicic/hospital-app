<Window x:Class="hospital.View.SecretaryHomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hospital.View"
        mc:Ignorable="d"
        Title="SecretaryHomeWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" HorizontalAlignment="Center" VerticalAlignment="Top">
            <Button Margin="5" Padding="5" Content="Handling account" Click="Button_Click"></Button>
            <Button x:Name="btnHandMedRecord" Margin="5" Padding="5" Content="Handling medical record" Click="btnHandMedRecord_Click"></Button>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
