<Window x:Class="hospital.View.DoctorHomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hospital.View"
        mc:Ignorable="d"
        Title="DoctorHomeWindow" Height="450" Width="800">
    <Grid >
        <Button Content="Appointments" HorizontalAlignment="Left" Margin="46,52,0,0" VerticalAlignment="Top" Width="95" Click="Appointment_Click"/>
        <Button Content="Medical Records" HorizontalAlignment="Left" Margin="46,116,0,0" VerticalAlignment="Top" Width="95" Click="MedicalRecord_Click"/>
        <Button Content="Therapy" HorizontalAlignment="Left" Margin="46,173,0,0" VerticalAlignment="Top" Width="95" Click="Therapy_Click"/>
        <Button Content="Logout" x:Name="btnLogout" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="688,52,0,0" Click="btnLogout_Click"/>

    </Grid>
</Window>
